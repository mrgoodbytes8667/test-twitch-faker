{
    "name": "mrgoodbytes8667/test-twitch-faker",
    "type": "library",
    "description": "Twitch Faker provider",
    "require": {
        "php": "^8.0",
        "mrgoodbytes8667/test-common-faker": "^0.3.5"
    },
    "require-dev": {
        "mrgoodbytes8667/twitch-response-bundle": ">=0.4",
        "symfony/test-pack": "^1.0.9"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "mrgoodbytes8667/twitch-response-bundle": "Necessary for actual usage",
        "mrgoodbytes8667/test-common": "Common helper traits for PHPUnit with Symfony",
        "symfony/phpunit-bridge": "Integrate Symfony with PHPUnit"
    },
    "conflict": {
        "phpunit/phpunit": "<9.5"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\Common\\Faker\\Providers\\": "Providers/",
            "Bytes\\Common\\Faker\\Twitch\\": "src/",
            "Bytes\\Common\\Faker\\Tests\\": "Tests/"
        }
    }
}
